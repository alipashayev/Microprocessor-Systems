void setup(){
  Serial.begin(9600);
  uint8_t sum;
  asm volatile(
    "ldi r19,1 \n\t" // counter
    "ldi r20,0 \n\t" // sum
    "ldi r21,11 \n\t" // limit
    "loop_start: \n\t"
    "add r20,r19 \n\t" // add counter to sum and store the result in sum
    "inc r19 \n\t"
    "cp r19,r21 \n\t"
    "brne loop_start \n\t"
    "mov %0, r20 \n\t"
    :"=r"(sum)
    :
    :"r19","r20","r21" 
  );
  Serial.println(sum);
}
void loop(){}
